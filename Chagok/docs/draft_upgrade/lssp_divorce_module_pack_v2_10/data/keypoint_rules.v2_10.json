{
  "version": "v2.10",
  "rules": [
    {
      "evidence_type": "CHAT_EXPORT",
      "kind": "ADMISSION",
      "name": "부정행위 인정 키워드",
      "pattern": "(외도|바람|불륜|상간|만났어|관계)",
      "flags": "i",
      "value_template": {
        "text": "${match}"
      },
      "ground_tags": [
        "G1"
      ],
      "base_confidence": 0.65,
      "base_materiality": 85
    },
    {
      "evidence_type": "CHAT_EXPORT",
      "kind": "THREAT",
      "name": "위협 표현",
      "pattern": "(죽여|가만 안 둬|찾아간다|끝장)",
      "flags": "i",
      "value_template": {
        "text": "${match}"
      },
      "ground_tags": [
        "G3"
      ],
      "base_confidence": 0.6,
      "base_materiality": 80
    },
    {
      "evidence_type": "CHAT_EXPORT",
      "kind": "SEPARATION",
      "name": "별거/가출 선언",
      "pattern": "(나간다|집 나갈게|별거|따로 살자|짐 뺄게)",
      "flags": "i",
      "value_template": {
        "text": "${match}"
      },
      "ground_tags": [
        "G2",
        "G6"
      ],
      "base_confidence": 0.62,
      "base_materiality": 75
    },
    {
      "evidence_type": "CHAT_EXPORT",
      "kind": "SPENDING",
      "name": "돈/송금 언급",
      "pattern": "(송금|입금|계좌|생활비|카드값|대출|돈(을)?\\\\s*줘)",
      "flags": "i",
      "value_template": {
        "text": "${match}"
      },
      "ground_tags": [
        "G2",
        "G6"
      ],
      "base_confidence": 0.55,
      "base_materiality": 60
    },
    {
      "evidence_type": "MEDICAL_RECORD",
      "kind": "MEDICAL",
      "name": "진단/치료 키워드",
      "pattern": "(진단서|치료|입원|상해|불안|우울)",
      "flags": "i",
      "value_template": {
        "text": "${match}"
      },
      "ground_tags": [
        "G3",
        "G4"
      ],
      "base_confidence": 0.75,
      "base_materiality": 90
    },
    {
      "evidence_type": "POLICE_RECORD",
      "kind": "POLICE_CASE",
      "name": "사건번호 패턴",
      "pattern": "([0-9]{2,4}[- ]?[0-9]{2,6}[- ]?[0-9]{2,6})",
      "flags": "",
      "value_template": {
        "ref_no": "${match}"
      },
      "ground_tags": [
        "G3",
        "G4",
        "G5"
      ],
      "base_confidence": 0.8,
      "base_materiality": 95
    },
    {
      "evidence_type": "POLICE_RECORD",
      "kind": "DATE_EVENT",
      "name": "신고일(YYYY.MM.DD)",
      "pattern": "((19|20)\\\\d{2}[./-](0[1-9]|1[0-2])[./-](0[1-9]|[12]\\\\d|3[01]))",
      "flags": "",
      "value_template": {
        "date": "${match}"
      },
      "ground_tags": [
        "G3",
        "G4",
        "G5"
      ],
      "base_confidence": 0.75,
      "base_materiality": 80
    },
    {
      "evidence_type": "CARD_STATEMENT",
      "kind": "SPENDING",
      "name": "금액(원)",
      "pattern": "([0-9]{1,3}(?:,[0-9]{3})+\\\\s*원)",
      "flags": "",
      "value_template": {
        "amount_text": "${match}"
      },
      "ground_tags": [
        "G1",
        "G6"
      ],
      "base_confidence": 0.6,
      "base_materiality": 65
    },
    {
      "evidence_type": "BANK_TX",
      "kind": "SPENDING",
      "name": "금액(원)",
      "pattern": "([0-9]{1,3}(?:,[0-9]{3})+\\\\s*원)",
      "flags": "",
      "value_template": {
        "amount_text": "${match}"
      },
      "ground_tags": [
        "G2",
        "G6"
      ],
      "base_confidence": 0.6,
      "base_materiality": 65
    }
  ]
}